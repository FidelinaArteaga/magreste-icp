{
  "canisters": {
    "magreste_labs_backend": {
      "main": "magreste_labs_backend/main.mo",
      "type": "motoko",
      "declarations": {
        "output": "src/declarations/magreste_labs_backend",
        "node_compatibility": true
      }
    },
    "magreste_labs_frontend": {
      "dependencies": ["magreste_labs_backend"],
      "source": ["src/magreste_labs_frontend/dist"],
      "type": "assets",
      "declarations": {
        "output": "src/declarations/magreste_labs_frontend"
      }
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    },
    "start": {
      "host": "127.0.0.1",
      "port": 4943
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  },
  "version": 1
}